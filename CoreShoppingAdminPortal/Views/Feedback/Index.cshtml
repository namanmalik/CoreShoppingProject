@model IEnumerable<CoreEcommerceUserPanal.Models.Feedback>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<style>
    .btn1 {
        margin-right: 3500px;
        margin-top: -10px;
    }
</style>

<center><h2>FeedBack Index</h2></center>
&nbsp;&nbsp;
<br />
<table class="table">
    <thead>

        <tr>
            <th>@Html.DisplayNameFor(model => model.FeedbackId)</th>
            <th>@Html.DisplayNameFor(model => model.Name)</th>
            <th>@Html.DisplayNameFor(model => model.EmaidId)</th>
            <th>@Html.DisplayNameFor(model => model.PhoneNo)</th>
            <th>@Html.DisplayNameFor(model => model.Comment)</th>



            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var pro in Model)
        {

